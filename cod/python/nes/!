from abc import abstractproperty, abstractmethod,ABC

class MemoryOwnerMixin(ABC):
    @abstractproperty
    def mem_start(self):
        pass

    @abstractproperty
    def mem_end(self):
        pass

    @abstractmethod
    def get_memory(self):
        pass

    def get(self, position, size):
        return self.get_memory()[position:position + 1]

    def set(self, position, value):
        self.get_memory()[position] = value
